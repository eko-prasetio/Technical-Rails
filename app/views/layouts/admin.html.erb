<!DOCTYPE html>
<html>
  <head>
    <title>Pretest</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "adminlte", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "all", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "select2.min", "data-turbo-track": "reload" %>
  </head>
  <body class="hold-transition sidebar-mini">
    <div class="wrapper">
      <%= render "base/header" %>
      <%= render "base/sidebar" %>
      <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
          <!-- Content Header (Page header) -->
          <div class="content-header">
            <div class="container-fluid">
              <div class="row mb-2">
                <div class="col-sm-6">
                  <h1 class="m-0">Starter Page</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                  <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Starter Page</li>
                  </ol>
                </div><!-- /.col -->
              </div><!-- /.row -->
            </div><!-- /.container-fluid -->
          </div>
          <!-- /.content-header -->

          <!-- Main content -->
          <div class="content">
            <div class="container-fluid">   
              <%= render "base/messages" %>
              <%= yield %>
            </div><!-- /.container-fluid -->
          </div>
          <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
    </div>
    <%= render "base/footer" %>
    <%= render "base/control_sidebar" %>
    <%= javascript_include_tag "application", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "jquery", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "adminlte", "data-turbo-track": "reload", defer: true %>
  </body>
</html>
